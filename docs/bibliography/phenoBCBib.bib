	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Way, Montgomery/Plant, Cell and Environment/Way, Montgomery - 2015 - Photoperiod constraints on tree phenology, performance and migration in a warming world.pdf:pdf},
	isbn = {0140-7791},
	issn = {13653040},
	journal = {Plant, Cell and Environment},
	keywords = {Bud burst,Bud set,Carbon uptake,Chilling,Climate change,Day length,Dormancy,Photosynthesis,Senescence},
	number = {9},
	pages = {1725--1736},
	pmid = {25142260},
	title = {{Photoperiod constraints on tree phenology, performance and migration in a warming world}},
