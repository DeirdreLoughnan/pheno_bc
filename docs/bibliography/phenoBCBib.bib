@article{Basler2014,
	abstract = {Spring phenology of temperate forest trees is optimized to maximize the length of the growing season while minimizing the risk of freezing damage. The release from winter dormancy is environmentally mediated by species-specific responses to temperature and photoperiod. We investigated the response of early spring phenology to temperature and photoperiod at different stages of dormancy release in cuttings from four temperate tree species in controlled environments. By tracking bud development, we were able to identify the onset of bud swelling and bud growth in Acer pseudoplatanus L., Fagus sylvatica L., Quercus petraea (Mattuschka) Liebl. and Picea abies (L.) H. Karst. At a given early stage of dormancy release, the onset and duration of the bud swelling prior to bud burst are driven by concurrent temperature and photoperiod, while the maximum growth rate is temperature dependent only, except for Fagus, where long photoperiods also increased bud growth rates. Similarly, the later bud burst was controlled by temperature and photoperiod (in the photoperiod sensitive species Fagus, Quercus and Picea). We conclude that photoperiod is involved in the release of dormancy during the ecodormancy phase and may influence bud burst in trees that have experienced sufficient chilling. This study explored and documented the early bud swelling period that precedes and defines later phenological stages such as canopy greening in conventional phenological works. It is the early bud growth resumption that needs to be understood in order to arrive at a causal interpretation and modelling of tree phenology at a large scale. Classic spring phenology events mark visible endpoints of a cascade of processes as evidenced here.},
	author = {Basler, David and K{\"{o}}rner, Christian},
	doi = {10.1093/treephys/tpu021},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Basler, K{\"{o}}rner/Tree Physiology/Basler, K{\"{o}}rner - 2014 - Photoperiod and temperature responses of bud swelling and bud burst in four temperate forest tree species.pdf:pdf},
	isbn = {0168-1923},
	issn = {17584469},
	journal = {Tree Physiology},
	keywords = {day length,deciduous trees,development,phenology,warming},
	number = {4},
	pages = {377--388},
	pmid = {24713858},
	title = {{Photoperiod and temperature responses of bud swelling and bud burst in four temperate forest tree species}},
	volume = {34},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1093/treephys/tpu021}}

@article{Basler2012,
	abstract = {The timing of spring phenology of trees reflects a trade-off between a longer growing season and a lower risk for damage by late freezing events. Temperature is driving rates of development directly, but given the high inter-annual variability in weather, it is a poor environmental cue for the progression of the season and thus, the period with low freezing risk. In contrast, photoperiod is a reliable and weather independent signal of the progression of the season. Using growth chamber experiments we assessed the photoperiod sensitivity of bud burst under artificial spring conditions in cuttings of 14 common European tree species that belong to different life-strategy types (pioneers or exotic species vs. native late-successional species; 3 conifers/11 broadleaved). Fully chilled twigs were sampled from populations along two elevational gradients in the Swiss Alps. Applying realistic contrasts in photoperiod, short photoperiods delayed bud burst in five late successional species to variable degree, whereas no distinct photoperiod sensitivity was observed in early successional species. In Picea abies, the photoperiod response was additionally influenced by elevation of origin, whereas in Quercus petraea and Abies alba regional differences in the photoperiod response were observed. For late successional species, photoperiod is thus an important environmental signal that will constrain responses to climatic warming because rising temperatures will drive phenology toward the species specific photoperiod threshold. {\textcopyright} 2012 Elsevier B.V.},
	annote = {Last paragraph before conclusion:
Reference otehr unknown cases where cuttings don't reflect tree responses--{\textgreater} if unknown why do we think it is an issue?},
	author = {Basler, David and K{\"{o}}rner, Christian},
	doi = {10.1016/j.agrformet.2012.06.001},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Basler, K{\"{o}}rner/Agricultural and Forest Meteorology/Basler, K{\"{o}}rner - 2012 - Photoperiod sensitivity of bud burst in 14 temperate forest tree species.pdf:pdf},
	isbn = {0168-1923},
	issn = {01681923},
	journal = {Agricultural and Forest Meteorology},
	keywords = {Daylength,Development,Season,Spring phenology,Temperature},
	pages = {73--81},
	publisher = {Elsevier B.V.},
	title = {{Photoperiod sensitivity of bud burst in 14 temperate forest tree species}},
	url = {http://dx.doi.org/10.1016/j.agrformet.2012.06.001},
	volume = {165},
	year = {2012},
	bdsk-url-1 = {http://dx.doi.org/10.1016/j.agrformet.2012.06.001}}

@article{Caffarra2011,
	abstract = {The process of adaptation is the result of stabilising selection caused by two opposite forces: protection against an unfavourable season (survival adaptation), and effective use of growing resources (capacity adaptation). As plant species have evolved different life strategies based on different trade offs between survival and capacity adaptations, different phenological responses are also expected among species. The aim of this study was to compare budburst responses of two opportunistic species (Betula pubescens, and Salix x smithiana) with that of two long-lived, late successional species (Fagus sylvatica and Tilia cordata) and consider their ecological significance. Thus, we performed a series of experiments whereby temperature and photoperiod were manipulated during dormancy. T. cordata and F. sylvatica showed low rates of budburst, high chilling requirements and responsiveness to light intensity, while B. pubescens and S. x smithiana had high rates of budburst, low chilling requirements and were not affected by light intensity. In addition, budburst in B. pubescens and S. x smithiana was more responsive to high forcing temperatures than in T. cordata and F. sylvatica. These results suggest that the timing of growth onset in B. pubescens and S. x smithiana (opportunistic) is regulated through a less conservative mechanism than in T. cordata and F. sylvatica (long-lived, late successional), and that these species trade a higher risk of frost damage for the opportunity of vigorous growth at the beginning of spring, before canopy closure. This information should be considered when assessing the impacts of climate change on vegetation or developing phenological models.},
	author = {Caffarra, Amelia and Donnelly, Alison},
	doi = {10.1007/s00484-010-0386-1},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Caffarra, Donnelly/International Journal of Biometeorology/Caffarra, Donnelly - 2011 - The ecological significance of phenology in four different tree species Effects of light and temperature on.pdf:pdf},
	isbn = {0020-7128},
	issn = {00207128},
	journal = {International Journal of Biometeorology},
	keywords = {Betula pubescens,Budburst,Fagus sylvatica,Life strategy,Photoperiod,Salix x smithiana,Temperature,Tilia cordata},
	number = {5},
	pages = {711--721},
	pmid = {21113629},
	title = {{The ecological significance of phenology in four different tree species: Effects of light and temperature on bud burst}},
	volume = {55},
	year = {2011},
	bdsk-url-1 = {https://doi.org/10.1007/s00484-010-0386-1}}

@article{Caffarra2011a,
	abstract = {ABSTRACT: The main factors triggering and releasing bud dormancy are photoperiod and temperature. Their individual and combined effects are complex and change along a transition from a dormant to a non-dormant state. Despite the number of studies reporting the effects of temperature and photoperiod on dormancy release and budburst, information on the parameters defining these relationships is scarce. The aim of the present study was to investigate the effects and interaction of temperature and photoperiod on the rates of dormancy induction and release in Betula pubescens (Ehrh.) in order to develop a conceptual model of budburst for this species. We performed a series of controlled environment experiments in which temperature and photoperiod were varied during different phases of dormancy in B. pubescens clones. Endodormancy was induced by short days and low temperatures, and released by exposure to a minimal period of chilling temperatures. Photoperiod during exposure to chilling temperatures did not affect budburst. Longer exposure to chilling increased growth capability (growth rate at a given forcing temperature) and decreased the time to budburst. During the forcing phase, budburst was promoted by photoperiods above a critical threshold, which was not constant, but decreased upon longer chilling exposures. These relationships between photoperiod and temperature have, as yet, not been integrated into the commonly used process-based phenological models. We suggest models should account for these relationships to increase the accuracy of their predictions under future climate conditions.},
	author = {Caffarra, Amelia and Donnelly, Alison and Chuine, Isabelle and Jones, Mike B.},
	doi = {10.3354/cr00980},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Caffarra et al/Climate Research/Caffarra et al. - 2011 - Modelling the timing of Betula pubescens budburst. I. Temperature and photoperiod A conceptual model.pdf:pdf},
	isbn = {0936-577X},
	issn = {0936577X},
	journal = {Climate Research},
	keywords = {Betula pubescens,Budburst,Controlled environment experiments,Dormancy,Phenology,Photoperiod,Temperature},
	number = {2},
	pages = {147--157},
	title = {{Modelling the timing of Betula pubescens budburst. I. Temperature and photoperiod: A conceptual model}},
	volume = {46},
	year = {2011},
	bdsk-url-1 = {https://doi.org/10.3354/cr00980}}

@article{Finn2007,
	abstract = {Biologishe Bundesanstalt, Bundessortenamt and CHemical Industry (BBCH) scales are a system for the uniform coding of phenologically similar growth stages of plants, using a two-digit decimal code. Currently, scales exist for a number of tree and woody species grown as crops; however, researchers are often forced to adapt existing scales or create their own when describing other tree or woody species growing as crops or weeds. This paper describes a general BBCH scale for the phenological growth stages of tree and woody species, including plant-specific descriptions and decimal codes covering vegetative and reproductive stages. The scale uses eight of the 10 principal BBCH stages, starting with sprouting/bud development (stage 0) and ending with dormancy (stage 9). This BBCH scale represents a single, standardised growth stage key for describing the growth of needle-bearing and leaf-bearing woody plants and promotes the adoption of the use of BBCH scales more widely. It is expected that the scale will form the basis for the creation of further scales specific for family, genus or species. {\textcopyright} 2007 Association of Applied Biologists.},
	author = {Finn, G. A. and Straszewski, A. E. and Peterson, V.},
	doi = {10.1111/j.1744-7348.2007.00159.x},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Finn, Straszewski, Peterson/Annals of Applied Biology/Finn, Straszewski, Peterson - 2007 - A general growth stage key for describing trees and woody plants.pdf:pdf},
	issn = {00034746},
	journal = {Annals of Applied Biology},
	keywords = {BBCH scale,Leaf bearing,Needle bearing,Phenology},
	number = {1},
	pages = {127--131},
	title = {{A general growth stage key for describing trees and woody plants}},
	volume = {151},
	year = {2007},
	bdsk-url-1 = {https://doi.org/10.1111/j.1744-7348.2007.00159.x}}

@article{Flynn2018,
	abstract = {Accurate predictions of spring plant phenology with climate change are critical for projec-tions of growing seasons, plant communities and a number of ecosystem services, including carbon storage. Progress towards prediction, however, has been slow because the major cues known to drive phenology -- temperature (including winter chilling and spring forcing) and photoperiod -- generally covary in nature and may interact, making accurate predictions of plant responses to climate change complex and nonlinear. Alternatively, recent work sug-gests many species may be dominated by one cue, which would make predictions much simpler. Here, we manipulated all three cues across 28 woody species from two North American forests. All species responded to all cues examined. Chilling exerted a strong effect, especially on budburst ({\`{A}}15.8 d), with responses to forcing and photoperiod greatest for leafout ({\`{A}}19.1 and {\`{A}}11.2 d, respectively). Interactions between chilling and forcing suggest that each cue may compensate somewhat for the other. Cues varied across species, leading to staggered leafout within each community and supporting the idea that phenology is a critical aspect of species' temporal niches. Our results suggest that predicting the spring phenology of communities will be difficult, as all species we studied could have complex, nonlinear responses to future warming.},
	author = {Flynn, D. F. B. and Wolkovich, E. M.},
	doi = {10.1111/nph.15232},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Flynn, Wolkovich/New Phytologist/Flynn, Wolkovich - 2018 - Temperature and photoperiod drive spring phenology across all species in a temperate forest community.pdf:pdf},
	issn = {0028646X},
	journal = {New Phytologist},
	keywords = {chilling,climate change,communities,daylength,forcing temperatures,forest,temporal niche,winter},
	pages = {1353--1362},
	title = {{Temperature and photoperiod drive spring phenology across all species in a temperate forest community}},
	url = {http://doi.wiley.com/10.1111/nph.15232},
	volume = {219},
	year = {2018},
	bdsk-url-1 = {http://doi.wiley.com/10.1111/nph.15232},
	bdsk-url-2 = {https://doi.org/10.1111/nph.15232}}

@article{Heide1993,
	abstract = {Dormancy release and thermal time to budburst as affected by duration of$\backslash$nchilling outdoors, followed by different flushing temperatures and$\backslash$ndaylengths in a phytotron, were studied in cuttings of several northern$\backslash$ntree species. In Betula pubescens, B. pendula and Prunus padus$\backslash$nvegetative buds were released from dormancy already in December, in$\backslash$nPopulus tremula in January, whereas in Alnus incana and A. glutinosa$\backslash$ndormancy was not released until February. Thermal time (day degrees$\backslash$n{\textgreater}0-degrees-C) to budburst decreased non-linearily with increasing$\backslash$nduration of chilling (i.e. duration outdoors), and the slope of this$\backslash$nrelationship differed among species. The estimated effective base$\backslash$ntemperature for accumulation of thermal time varied from +1-degrees-C in$\backslash$nP. tremula to -4-degrees-C in P. padus. The use of 0-degrees-C as base$\backslash$ntemperature is recommended. Long days reduced the thermal time to$\backslash$nbudburst at all flushing temperatures (9, 15 and 21-degrees-C) in all$\backslash$nthe above species and in Corylus avellana, whereas Sorbus aucuparia and$\backslash$nRubus idaeus showed no daylength response. Since the chilling$\backslash$nrequirement of all species was far exceeded even in a winter with$\backslash$nJanuary-March temperatures 6.5-degrees-C above normal, it is concluded$\backslash$nthat under Scandinavian conditions, the main effect of climatic warming$\backslash$nwould be earlier budburst and, associated with that, a longer growing$\backslash$nseason and increased risk of spring frost injury.},
	author = {Heide, O. M.},
	doi = {10.1034/j.1399-3054.1993.880401.x},
	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Heide/Physiologia Plantarum/Heide-1993-Physiologia{\_}Plantarum.pdf:pdf},
	issn = {0031-9317},
	journal = {Physiologia Plantarum},
	keywords = {- bud dormancy,agricultural univ,as,chilling,climatic warming,daylength,dept of biology and,heide,m,n-1432,nature conservation,northern trees,norway,o,of,po box 5014,temperature,thermal time},
	number = {4},
	pages = {531--540},
	title = {{Daylength and thermal time responses of budburst during dormancy release in some northern deciduous trees}},
	volume = {88},
	year = {1993},
	bdsk-url-1 = {https://doi.org/10.1034/j.1399-3054.1993.880401.x}}
