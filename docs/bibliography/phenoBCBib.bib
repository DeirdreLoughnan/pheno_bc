	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Polgar, Primack/New Phytologist/PolgarandPrimack.2011.pdf:pdf},
	issn = {0028646X},
	journal = {New Phytologist},
	keywords = {Budburst,Climate change,Leaf-out,Phenology,Temperate forests},
	number = {4},
	pages = {926--941},
	title = {{Leaf-out phenology of temperate woody plants: From trees to ecosystems}},
	volume = {191},
