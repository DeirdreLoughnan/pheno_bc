	issn = {1932-6203},
	journal = {PLoS ONE},
	language = {en},
	pages = {1-5},
	title = {Favorable {Climate} {Change} {Response} {Explains} {Non}-{Native} {Species}' {Success} in {Thoreau}'s {Woods}},
