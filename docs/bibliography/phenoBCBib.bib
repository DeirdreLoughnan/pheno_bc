	issn = {1466-822X, 1466-8238},
	journal = {Global Ecology and Biogeography},
	language = {en},
	month = jan,
	number = {2},
	pages = {275--285},
	shorttitle = {Phylogenetically weighted regression},
	title = {Phylogenetically weighted regression: {A} method for modelling non‐stationarity on evolutionary trees},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/geb.12841},
