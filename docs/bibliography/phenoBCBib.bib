	file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Zohner, Renner/Ecology Letters/Zohner, Renner - 2014 - Common garden comparison of the leaf-out phenology of woody species from different native climates, combined wit.pdf:pdf},
	isbn = {1461-0248},
	issn = {14610248},
