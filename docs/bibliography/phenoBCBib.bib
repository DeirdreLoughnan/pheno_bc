	issn = {0022-0477, 1365-2745},
	journal = {Journal of Ecology},
	journaltitle = {Journal of Ecology},
	langid = {english},
	pages = {156--169},
	shortjournal = {Journal of Ecology},
	title = {Higher‐latitude spring‐flowering herbs advance their phenology more than trees with warming temperatures},
	url = {https://besjournals.onlinelibrary.wiley.com/doi/10.1111/1365-2745.14023},
	urldate = {2023-11-10},
	volume = {111},
	bdsk-url-1 = {https://besjournals.onlinelibrary.wiley.com/doi/10.1111/1365-2745.14023},
	bdsk-url-2 = {https://doi.org/10.1111/1365-2745.14023}}

@article{Post2018,
	abstract = {Abstract
            In the Northern Hemisphere, springtime events are frequently reported as advancing more rapidly at higher latitudes, presumably due to an acceleration of warming with latitude. However, this assumption has not been investigated in an analytical framework that simultaneously examines acceleration of warming with latitude while accounting for variation in phenological time series characteristics that might also co-vary with latitude. We analyzed 743 phenological trend estimates spanning 86 years and 42.6 degrees of latitude in the Northern Hemisphere, as well as rates of Northern Hemisphere warming over the same period and latitudinal range. We detected significant patterns of co-variation in phenological time series characteristics that may confound estimates of the magnitude of variation in trends with latitude. Notably, shorter and more recent time series tended to produce the strongest phenological trends, and these also tended to be from higher latitude studies. However, accounting for such variation only slightly modified the relationship between rates of phenological advance and latitude, which was highly significant. Furthermore, warming has increased non-linearly with latitude over the past several decades, most strongly since 1998 and northward of 59$\,^{\circ}$N latitude. The acceleration of warming with latitude has likely contributed to an acceleration of phenological advance along the same gradient.},
	author = {Post, Eric and Steinman, Byron A. and Mann, Michael E.},
	date = {2018-03-02},
	date-added = {2023-11-09 16:36:52 -0800},
	date-modified = {2023-11-09 16:54:32 -0800},
	doi = {10.1038/s41598-018-22258-0},
	file = {Post et al. - 2018 - Acceleration of phenological advance and warming w.pdf:/Users/deirdreloughnan/Zotero/storage/26UD4PJ8/Post et al. - 2018 - Acceleration of phenological advance and warming w.pdf:application/pdf},
	issn = {2045-2322},
	journal = {Scientific Reports},
	journaltitle = {Scientific Reports},
	langid = {english},
	number = {3927},
	pages = {1-8},
	shortjournal = {Sci Rep},
	title = {Acceleration of phenological advance and warming with latitude over the past century},
	url = {https://www.nature.com/articles/s41598-018-22258-0},
	urldate = {2023-11-10},
	volume = {8},
	year = {2018},
	bdsk-url-1 = {https://www.nature.com/articles/s41598-018-22258-0},
